<Window x:Class="Vs_Toemrer_Lagerstyringssystemsprojekt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Vs_Toemrer_Lagerstyringssystemsprojekt" xmlns:local1="clr-namespace:Vs_Toemrer_Lagerstyringssystemsprojekt.Viewmodel"
        mc:Ignorable="d"
        Title="MainWindow"
        Height="500"
        Width="800"
        WindowStartupLocation="CenterScreen"
        UseLayoutRounding="True"
        SnapsToDevicePixels="True">
    <DockPanel x:Name="ViewDockPanel">
        <StackPanel x:Name="MainViewStackPanel" DockPanel.Dock="Left" Width="100" Background="LightGray">
            <Button x:Name="btnProjects" Content="Projekter" Click="Projects_Click" Height="50"/>
            <Button x:Name="btnStorage" Content="Lager" Click="Storage_Click" Height="50"/>
            <Button x:Name="btnRegistry" Content="Register" Click="Registry_Click" Height="50"/>
        </StackPanel>
        <ContentControl x:Name="MainContent"/>
    </DockPanel>

</Window>
