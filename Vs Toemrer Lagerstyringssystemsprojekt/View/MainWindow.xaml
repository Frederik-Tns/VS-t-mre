<Window x:Class="Vs_Toemrer_Lagerstyringssystemsprojekt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Vs_Toemrer_Lagerstyringssystemsprojekt"
        mc:Ignorable="d"
        Title="MainWindow"
        Height="500"
        Width="800"
        WindowStartupLocation="CenterScreen"
        UseLayoutRounding="True"
        SnapsToDevicePixels="True">
    <DockPanel>
        <StackPanel DockPanel.Dock="Left" Width="100" Background="LightGray">
            <Button x:Name="Projekter" Content="Projekter" Click="Projekter_Click" Height="50"/>
            <Button x:Name="Lager" Content="Lager" Click="Lager_Click" Height="50"/>
            <Button x:Name="Register" Content="Register" Click="Register_Click" Height="50"/>
        </StackPanel>
        <ContentControl x:Name="MainContent" />
    </DockPanel>
</Window>
